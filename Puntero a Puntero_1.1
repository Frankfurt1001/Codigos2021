#include <stdio.h>
//Puntero a puntero
void imprimir (int *p){//los arreglos pasan como punteros al darselas a las funciones
	
	for(int i = 0; i < 5; i++)
		printf("%d\n", p[i]);
	
	p[0] = 10;
	
}
int main(int argc, char *argv[]) {
	int arreglo [5]= {1,2,3,4,5};
	imprimir(arreglo);

	
	return 0;
}

